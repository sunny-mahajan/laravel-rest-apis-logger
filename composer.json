{
  "autoload": {
    "psr-4": {
      "TF\\": "src/"
    }
  },
  "name": "tfspl/restlogger",
  "description": "Small laravel package for viewing rest api logs for debugging.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Sunny Kumar",
      "email": "sunny@techformation.co.in"
    }
  ],
  "extra": {
    "laravel": {
      "providers": [
        "TF\\Providers\\RestLogsServiceProvider"
      ]
    }
  },
  "require": {
    "php": ">=5.6",
    "laravel/framework": ">=5.5",
    "laravel/sanctum": ">=2.11",
    "laravel/tinker": ">=2.5",
    "predis/predis": ">=1.1",
    "ext-intl": "*",
    "ext-mbstring": "*"
  },
  "require-dev": {
    "mockery/mockery": ">=1.2",
    "phpunit/phpunit": ">=9.0"
  },
  "minimum-stability": "stable"
}
